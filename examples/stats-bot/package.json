{
  "name": "mocha-stats-bot-example",
  "version": "1.0.0",
  "description": "Example Discord stats bot using Mocha library",
  "main": "bot.ts",
  "scripts": {
    "dev": "bun --watch bot.ts",
    "start": "bun bot.ts",
    "build": "bun build bot.ts --outdir dist",
    "deploy": "bun deploy-commands.ts",
    "cleanup": "bun deploy-commands.ts cleanup",
    "cleanup:global": "bun deploy-commands.ts cleanup --global",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type-check": "bunx tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "discord",
    "bot",
    "stats",
    "mocha"
  ],
  "author": "Your Name",
  "license": "MIT"
}
